Create("!goto bank", function() Stand.Action = "Crew" Play(8655611378, true) if OWNER.Character.BodyEffects["K.O"].Value == false then Crew(false) Equip("Combat") Chat("Bringing to bank!") Stand.Action = "BankTP" elseif OWNER.Character.BodyEffects["K.O"].Value == true then Stand.Action = "Summoned" wait(0.5) Chat("TP commands won't work when knocked!") end end)
